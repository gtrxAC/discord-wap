<% if (messages.length == messageCount) { %>
    <form action="/wap/ch<%= reverseChat ? '#bottom' : '' %>">
        <input type="hidden" name="id" value="<%= id %>">
        <input type="hidden" name="cname" value="<%= cname %>">
        <input type="hidden" name="before" value="<%= reverseChat ? messages[0].id : messages[messages.length - 1].id %>"/>
        <input type="hidden" name="page" value="<%= Number(page) + 1 %>"/>
        <input type="submit" value="Older messages">
    </form>
<% } %>